class Solution {
    public int removeDuplicates(int[] nums) {
        int n = nums.length ;
        if(n==1) return 1 ;
        int x = 1 ;
        for(int i=1;i<n;i++){
            if(nums[x-1]!=nums[i]){
                nums[x] = nums[i] ;
                x++ ;
            } 
        }
        return x ;
    }
}